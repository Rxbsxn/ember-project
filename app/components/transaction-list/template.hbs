{{#each flashMessages.queue as |flash|}}
  {{flash-message flash=flash}}
{{/each}}

{{#paper-list}}
  {{#each transactions as |item|}}
    {{#paper-item class="md-3-line" }}
      <div class="md-list-item-text">
        {{#link-to 'transactions.transaction' item.id}}
          <h3>Amount:{{ format-currency item.amount}}</h3>
        {{/link-to}}
        <h4>Date of create: {{item.createdAt}}</h4>
        {{#paper-button raised=true warn=true onClick=(action "removeTransaction" item) bubbles=false}}Delete{{/paper-button}}
      </div>
    {{/paper-item}}
  {{/each}}
{{/paper-list}}
